{
  "expo": {
    "extra": {
      "fonts": "assets/fonts/",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7e577f10-484b-4ecd-8286-ce379e20a6fb"
      },
      "API_URL": "http://localhost:8000",
      "ENV": "development"
    },
      }
    },
    "name": "Islamic Library",
    "slug": "islamic-library",
    "description": "A collection of Islamic books and resources",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "ayoubrached"
  }
}
